<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="warn" name="Sale" packages="">
	<Appenders>
		<Console name="STDIO" target="SYSTEM_OUT">
			<PatternLayout>
				<Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
			</PatternLayout>
		</Console>

	</Appenders>

	<Loggers>
		<Root level="warn">
			<AppenderRef ref="STDIO" />
		</Root>
	</Loggers>
</Configuration> 